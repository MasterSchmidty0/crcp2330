 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=nOutA);
    Not(in=b, out=nOutB);
    And(a=nOutA, b=nOutB, out=nIn);
    Not(in=nIn, out=out);
}